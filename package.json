{
  "name": "yt-dl",
  "description": "a wrapper for various youtube downloading solutions",
  "keywords": [
    "youtube",
    "downloading",
    "youtube-dl",
    "ytdl-core",
    "yt-dlp"
  ],
  "version": "0.0.1",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "vaaski <admin@vaa.ski>",
  "scripts": {
    "start": "node lib",
    "build": "rimraf lib && tsc",
    "prepare": "npm run build",
    "dev": "ts-node src",
    "test": "nyc ava",
    "coverage": "live-server coverage/lcov-report",
    "prepublishOnly": "npm test"
  },
  "files": [
    "lib/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "tests/**/*"
    ],
    "require": [
      "ts-node/register"
    ],
    "failFast": true
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "ava": "^3.15.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ava": "^13.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "live-server": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
